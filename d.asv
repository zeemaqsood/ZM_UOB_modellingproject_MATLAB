function dist = d(x, y)
    (x(:, 2) .* y(:, 1) == y(:, 2) .* x(:, 1))

    if x(:, 2) * y(:, 1) == y(:, 2) * x(:, 1)
        dist = sqrt((x(:, 1) - y(:, 1)).^2 + (x(:, 2) - y(:, 2)).^2) .* (x(:, 2) .* y(:, 1) == y(:, 2) .* x(:, 1));
        
    else
        dist = sqrt(x(:, 2).^2 + x(:, 1).^2) + sqrt(y(:, 2).^2 + y(:, 1).^2) .* ~(x(:, 2) .* y(:, 1) == y(:, 2) .* x(:, 1));
    end
end